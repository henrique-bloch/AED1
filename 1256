#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int M, C, x;
        scanf("%d %d", &M, &C);

        // listas: cada índice armazena uma sequência de ints
        int hash[101][201];  // M<=100, no máximo C=200 elementos
        int sz[101] = {0};

        for (int i = 0; i < M; i++) sz[i] = 0;

        for (int i = 0; i < C; i++) {
            scanf("%d", &x);
            int h = x % M;
            hash[h][sz[h]++] = x;
        }

        // imprimir
        for (int i = 0; i < M; i++) {
            printf("%d -> ", i);
            for (int j = 0; j < sz[i]; j++)
                printf("%d -> ", hash[i][j]);
            printf("\\\n");
        }

        if (T) printf("\n"); // linha em branco entre casos
    }

    return 0;
}
