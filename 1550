#include <stdio.h>
#include <string.h>

int f[20000], d[10000];
int ini, fim;

int inverte(int x) {
    int r = 0;
    while (x > 0) {
        r = r * 10 + (x % 10);
        x /= 10;
    }
    return r;
}

int bfs(int a, int b) {
    memset(d, -1, sizeof(d));
    ini = fim = 0;

    f[fim++] = a;
    d[a] = 0;

    while (ini < fim) {
        int u = f[ini++];

        if (u == b) return d[u];

        int v1 = u + 1;
        if (v1 < 10000 && d[v1] == -1) {
            d[v1] = d[u] + 1;
            f[fim++] = v1;
        }

        int v2 = inverte(u);
        if (v2 < 10000 && d[v2] == -1) {
            d[v2] = d[u] + 1;
            f[fim++] = v2;
        }
    }
    return -1;
}

int main() {
    int t, a, b;
    if (scanf("%d", &t) != 1) return 0;
    while (t--) {
        scanf("%d %d", &a, &b);
        printf("%d\n", bfs(a, b));
    }
    return 0;
}
