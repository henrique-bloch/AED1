#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAX_S 1000
#define MAX_L 1001

char lista[MAX_S][MAX_L];
int qtd = 0;

void gera(const char *orig, int idx, char *atual, int pos) {
    if (pos > 0) {
        atual[pos] = '\0';
        int existe = 0;
        for (int i = 0; i < qtd; i++) {
            if (strcmp(lista[i], atual) == 0) {
                existe = 1;
                break;
            }
        }
        if (!existe) {
            strcpy(lista[qtd++], atual);
        }
    }
    
    char uso[26] = {0};
    for (int i = idx; orig[i] != '\0'; i++) {
        if (uso[orig[i] - 'a']) continue;
        uso[orig[i] - 'a'] = 1;
        
        atual[pos] = orig[i];
        gera(orig, i + 1, atual, pos + 1);
    }
}

int comp(const void *a, const void *b) {
    return strcmp((char*)a, (char*)b);
}

int main() {
    char s[MAX_L], atual[MAX_L];
    
    while (scanf("%s", s) != EOF) {
        qtd = 0;
        gera(s, 0, atual, 0);
        
        qsort(lista, qtd, sizeof(lista[0]), comp);
        
        for (int i = 0; i < qtd; i++) {
            printf("%s\n", lista[i]);
        }
        printf("\n");
    }
    
    return 0;
}
