#include <stdio.h>
#include <string.h>

#define MAX_EXPR_LENGTH 1001

int verifica_parenteses(const char *expressao) {
    int balanceamento = 0;
    
    for (int i = 0; expressao[i] != '\0'; i++) {
        if (expressao[i] == '(') {
            balanceamento++;
        } else if (expressao[i] == ')') {
            balanceamento--;
            if (balanceamento < 0) {
                return 0;
            }
        }
    }
    
    return balanceamento == 0;
}

int main() {
    char expressao[MAX_EXPR_LENGTH];
    
    while (fgets(expressao, MAX_EXPR_LENGTH, stdin) != NULL) {
        size_t len = strlen(expressao);
        if (len > 0 && expressao[len - 1] == '\n') {
            expressao[len - 1] = '\0';
        }
        
        if (verifica_parenteses(expressao)) {
            printf("correct\n");
        } else {
            printf("incorrect\n");
        }
    }
    
    return 0;
}
