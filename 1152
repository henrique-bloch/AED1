#include <stdio.h>
#include <stdlib.h>

typedef long long ll;

typedef struct {
    int u, v, w;
} Aresta;

int comparar(const void *a, const void *b) {
    Aresta *e1 = (Aresta*)a;
    Aresta *e2 = (Aresta*)b;
    return (e1->w > e2->w) - (e1->w < e2->w);
}

int *pai;
int *rank;

int achar(int i) {
    if (pai[i] == i) return i;
    return pai[i] = achar(pai[i]);
}

void unir(int i, int j) {
    int raiz_i = achar(i);
    int raiz_j = achar(j);
    if (raiz_i != raiz_j) {
        if (rank[raiz_i] < rank[raiz_j]) pai[raiz_i] = raiz_j;
        else if (rank[raiz_i] > rank[raiz_j]) pai[raiz_j] = raiz_i;
        else {
            pai[raiz_i] = raiz_j;
            rank[raiz_j]++;
        }
    }
}

int main() {
    int m, n;
    while (scanf("%d %d", &m, &n) == 2 && (m != 0 || n != 0)) {
        Aresta *e = malloc(sizeof(Aresta) * n);
        ll total = 0;

        for (int i = 0; i < n; i++) {
            scanf("%d %d %d", &e[i].u, &e[i].v, &e[i].w);
            total += e[i].w;
        }

        qsort(e, n, sizeof(Aresta), comparar);

        pai = malloc(sizeof(int) * m);
        rank = malloc(sizeof(int) * m);
        for (int i = 0; i < m; i++) {
            pai[i] = i;
            rank[i] = 0;
        }

        ll mst_peso = 0;
        int cont = 0;
        for (int i = 0; i < n && cont < m - 1; i++) {
            if (achar(e[i].u) != achar(e[i].v)) {
                unir(e[i].u, e[i].v);
                mst_peso += e[i].w;
                cont++;
            }
        }

        printf("%lld\n", total - mst_peso);

        free(e);
        free(pai);
        free(rank);
    }
    return 0;
}
