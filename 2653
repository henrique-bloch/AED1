#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define TAM_HASH 200003

typedef struct No {
    char *s;
    struct No *prox;
} No;

No* tab[TAM_HASH];

unsigned long gera_hash(char *s) {
    unsigned long h = 5381;
    int c;
    while ((c = *s++)) h = ((h << 5) + h) + c;
    return h % TAM_HASH;
}

int por(char *str) {
    unsigned long h = gera_hash(str);
    No *atual = tab[h];
    while (atual) {
        if (strcmp(atual->s, str) == 0) return 0;
        atual = atual->prox;
    }
    No *novo = malloc(sizeof(No));
    novo->s = strdup(str);
    novo->prox = tab[h];
    tab[h] = novo;
    return 1;
}

int main() {
    char s[1000005];
    int total = 0;
    while (scanf("%s", s) != EOF) {
        total += por(s);
    }
    printf("%d\n", total);
    return 0;
}
